{
  "name": "@silintl/cover-ui",
  "scripts": {
    "prebuild:dev": "npm run clean:build",
    "build:dev": "rollup -c -w",
    "prebuild:prod": "echo CF_PAGES_BRANCH=${CF_PAGES_BRANCH} >> .env; echo GA_MEASUREMENT_ID=${GA_MEASUREMENT_ID} >> .env; echo FRESHDESK_ID=${FRESHDESK_ID} >> .env; echo API_HOST=${API_HOST} >> .env; npm run clean:build",
    "build:prod": "rollup -c",
    "check": "npx svelte-check",
    "clean": "run-p -l clean:build clean:deps",
    "clean:build": "rimraf dist/bundle* dist/workbox*",
    "clean:deps": "npm prune",
    "format": "prettier --write ./src/**/*.{js,ts,svelte}",
    "lint": "eslint ./src/**/*.{js,ts,svelte}",
    "lint:fix": "eslint --fix ./'src/**/*.{js,ts,svelte}'",
    "prelint": "npm run format",
    "local:dev": "run-p -l build:dev serve:http",
    "local:prod": "run-p -l build:prod serve:http",
    "serve:http": "sirv dist --dev --host 0.0.0.0 --port 8081 --single",
    "serve:https": "sirv dist --dev --host 0.0.0.0 --port 8443 --single --http2 --key key.pem --cert cert.pem"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@roxi/routify": "^2.18.8",
    "@silintl/ui-components": "^6.3.0",
    "@tsconfig/svelte": "^2.0.1",
    "@types/croppie": "^2.6.1",
    "@types/gtag.js": "^0.0.10",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.11.6",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "croppie": "^2.6.5",
    "date-fns": "^2.29.1",
    "eslint": "^7.32.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "lodash-es": "^4.17.21",
    "material-components-web": "^13.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "qs": "^6.11.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.0",
    "rollup-plugin-dotenv": "^0.3.0",
    "rollup-plugin-includepaths": "^0.2.4",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-polyfill-node": "^0.10.2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "1.50.x",
    "sirv-cli": "^1.0.14",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}

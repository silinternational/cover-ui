{
  "name": "@silintl/cover-ui",
  "engines": {
    "node": ">=18 <19",
    "comment": "Constrain Node version to match our staging and production environments"
  },
  "type": "module",
  "scripts": {
    "dev": "vite --port 8081",
    "build": "vite build",
    "preview": "vite preview",
    "prebuild:dev": "npm run clean:build",
    "prebuild:prod": "echo CF_PAGES_BRANCH=${CF_PAGES_BRANCH} >> .env; echo GA_MEASUREMENT_ID=${GA_MEASUREMENT_ID} >> .env; echo FRESHDESK_ID=${FRESHDESK_ID} >> .env; echo API_HOST=${API_HOST}; echo SENTRY_DSN=${SENTRY_DSN} >> .env; npm run clean:build",
    "check": "npx svelte-check",
    "clean": "conc \"npm run clean:build\" \"npm run clean:deps\"",
    "clean:build": "rimraf dist/bundle* dist/workbox*",
    "clean:deps": "npm prune",
    "format": "prettier --write ./src/**/*.{ts,svelte}",
    "lint": "eslint ./src/**/*.{ts,svelte}",
    "lint:fix": "eslint --fix ./'src/**/*.{ts,svelte}'",
    "prelint": "npm run format"
  },
  "dependencies": {
    "@roxi/routify": "^3.0.0-next.223",
    "@sentry/svelte": "^7.111.0",
    "@silintl/ui-components": "^11.3.0",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@tailwindcss/typography": "^0.5.10",
    "@tsconfig/svelte": "^5.0.2",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.1",
    "croppie": "^2.6.5",
    "date-fns": "^2.30.0",
    "iconify-icon": "^1.0.8",
    "lodash-es": "^4.17.21",
    "material-components-web": "^14.0.0",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^12.0.1",
    "qs": "^6.11.2",
    "svelte": "^4",
    "svelte-preprocess": "^5",
    "tailwind-utopia": "github:domchristie/tailwind-utopia",
    "tailwindcss": "^3.3.3",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@types/croppie": "^2.6.1",
    "@types/gtag.js": "^0.0.12",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.12.7",
    "@types/qs": "^6.9.8",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-plugin-svelte": "^2.35.1",
    "prettier": "2.8.8",
    "prettier-plugin-svelte": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.4",
    "rimraf": "^3.0.2",
    "sirv-cli": "^2.0.2",
    "svelte-check": "^3.6.4",
    "tslib": "^2.6.2",
    "vite": "^5.2.0"
  }
}

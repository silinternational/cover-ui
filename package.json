{
  "name": "@silintl/cover-ui",
  "engines": {
    "node": ">=18 <19",
    "comment": "Constrain Node version to match our staging and production environments"
  },
  "scripts": {
    "prebuild:dev": "npm run clean:build",
    "build:dev": "rollup -c -w",
    "prebuild:prod": "echo CF_PAGES_BRANCH=${CF_PAGES_BRANCH} >> .env; echo GA_MEASUREMENT_ID=${GA_MEASUREMENT_ID} >> .env; echo FRESHDESK_ID=${FRESHDESK_ID} >> .env; echo API_HOST=${API_HOST} >> .env; npm run clean:build",
    "build:prod": "rollup -c",
    "check": "npx svelte-check",
    "clean": "conc \"npm run clean:build\" \"npm run clean:deps\"",
    "clean:build": "rimraf dist/bundle* dist/workbox*",
    "clean:deps": "npm prune",
    "format": "prettier --write ./src/**/*.{js,ts,svelte}",
    "lint": "eslint ./src/**/*.{js,ts,svelte}",
    "lint:fix": "eslint --fix ./'src/**/*.{js,ts,svelte}'",
    "prelint": "npm run format",
    "local:dev": "conc \"npm run build:dev\" \"npm run serve:http\"",
    "local:prod": "conc \"npm run build:prod\" \"npm run serve:http\"",
    "bun:dev": "conc \"bun run build:dev\" \"bun run serve:http\"",
    "bun:prod": "conc \"bun run build:prod\" \"bun run serve:http\"",
    "serve:http": "sirv dist --dev --host 0.0.0.0 --port 8081 --single",
    "serve:https": "sirv dist --dev --host 0.0.0.0 --port 8443 --single --http2 --key key.pem --cert cert.pem"
  },
  "devDependencies": {
    "material-components-web": "^14.0.0",
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-html": "^1.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@roxi/routify": "^2.18.12",
    "@silintl/ui-components": "^10.4.0",
    "@tsconfig/svelte": "^2.0.1",
    "@types/croppie": "^2.6.1",
    "@types/gtag.js": "^0.0.12",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^16.11.6",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "concurrently": "^8.2.1",
    "croppie": "^2.6.5",
    "date-fns": "^2.30.0",
    "eslint": "^7.32.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "iconify-icon": "^1.0.8",
    "lodash-es": "^4.17.21",
    "material-components-web": "^14.0.0",
    "postcss": "^8.4.23",
    "prettier": "2.8.8",
    "prettier-plugin-svelte": "^2.10.0",
    "qs": "^6.11.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-dotenv": "^0.4.1",
    "rollup-plugin-includepaths": "^0.2.4",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^7.1.4",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "1.55.x",
    "sirv-cli": "^2.0.2",
    "svelte": "^3.55.1",
    "svelte-check": "^2.9.0",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.5.0",
    "typescript": "^4.9.4"
  }
}
